<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <system.serviceModel>
    <services>
      <service name="Wcf.UserService" behaviorConfiguration="UserServiceBehaviors">
        <endpoint address="" binding="basicHttpBinding" contract="Wcf.IUserService"/>
      </service>
      <service name="Wcf.Service1" behaviorConfiguration="UserServiceBehaviors">
        <endpoint address="" binding="basicHttpBinding" contract="Wcf.IService1"/>
      </service>
     <service name="Wcf.Service2" behaviorConfiguration="UserServiceBehaviors">
        <endpoint address="" binding="wsDualHttpBinding" contract="Wcf.IService2"/>
      </service>
    </services>
    <behaviors>
      <serviceBehaviors>
        <behavior name="UserServiceBehaviors" >
          <!-- 将下列元素添加到服务行为配置中。 -->
          <serviceMetadata httpGetEnabled="true" />
        </behavior>
        <!--<behavior name="Service1Behaviors" >
          --><!-- 将下列元素添加到服务行为配置中。 --><!--
          <serviceMetadata httpGetEnabled="true" />
        </behavior>-->
        <behavior name="Service2Behaviors" >
          <!-- 将下列元素添加到服务行为配置中。 -->
          <serviceMetadata httpGetEnabled="true" />
        </behavior>
      </serviceBehaviors>
    </behaviors>
  </system.serviceModel>

</configuration>
